{
  "name": "jasminecms/jasmine",
  "description": "A Laravel based CMS.",
  "license": "MIT",
  "authors": [
    {
      "name": "Mordi Sacks"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "alexusmai/laravel-file-manager": "^3.1.1",
    "laravel/framework": "^11.0",
    "laravel/ui": "^4.0",
    "spatie/laravel-translatable": "^6.7",
    "inertiajs/inertia-laravel": "^1.0",
    "tightenco/ziggy": "^1.5",
    "composer-runtime-api": "^2.2",
    "pragmarx/google2fa": "^8.0",
    "spatie/eloquent-sortable": "^4.4"
  },
  "require-dev": {
    "laravel/pint": "^1.14",
    "nunomaduro/collision": "^8.1.1",
    "larastan/larastan": "^2.9",
    "orchestra/testbench": "^9.0.0",
    "pestphp/pest": "^2.35",
    "pestphp/pest-plugin-arch": "^2.7",
    "pestphp/pest-plugin-laravel": "^2.3",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan-deprecation-rules": "^1.1",
    "phpstan/phpstan-phpunit": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Jasmine\\Jasmine\\": "src/",
      "Jasmine\\Jasmine\\Database\\Factories\\": "database/factories/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Jasmine\\Jasmine\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "format": "vendor/bin/pint"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jasmine\\Jasmine\\JasmineServiceProvider"
      ],
      "aliases": {
        "Jasmine": "Jasmine\\Jasmine\\Facades\\Jasmine"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  }
}
