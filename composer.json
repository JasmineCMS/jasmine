{
  "name": "jasminecms/jasmine",
  "description": "A Laravel based CMS.",
  "license": "MIT",
  "authors": [
    {
      "name": "Mordi Sacks"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "alexusmai/laravel-file-manager": "^3.1",
    "laravel/framework": "^11.20|^12.0",
    "laravel/ui": "^4.0",
    "spatie/laravel-translatable": "^6.7",
    "inertiajs/inertia-laravel": "^2.0",
    "tightenco/ziggy": "^1.5",
    "composer-runtime-api": "^2.2",
    "pragmarx/google2fa": "^8.0",
    "spatie/eloquent-sortable": "^4.4"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Jasmine\\Jasmine\\": "src/",
      "Jasmine\\Jasmine\\Database\\Factories\\": "database/factories/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jasmine\\Jasmine\\JasmineServiceProvider"
      ],
      "aliases": {
        "Jasmine": "Jasmine\\Jasmine\\Facades\\Jasmine"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
