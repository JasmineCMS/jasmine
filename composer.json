{
    "name": "jasminecms/jasmine",
    "description": "A Laravel based CMS.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mordi Sacks"
        }
    ],
    "require": {
        "php": "^8.0",
        "alexusmai/laravel-file-manager": "^2.5",
        "laravel/framework": "^8.0",
        "laravel/ui": "^3.0",
        "spatie/laravel-translatable": "^4.3",
        "yajra/laravel-datatables-oracle": "^9.9",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Jasmine\\Jasmine\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jasmine\\Jasmine\\JasmineServiceProvider"
            ],
            "aliases": {
                "Jasmine": "Jasmine\\Jasmine\\Facades\\Jasmine"
            }
        }
    }
}
